  <Operation name="asin" exec="SAME_UN" syntax="Function" commutative="false" inputTailRepeat="0">
    <SameUn onlyDomainIll="std::fabs(v)&gt;1" function="(T)ScaleRad(std::asin(v))" foTypes="f"/>
    <Result>
      <Field spatial="Either">
        <DataType value="Directional"/>
