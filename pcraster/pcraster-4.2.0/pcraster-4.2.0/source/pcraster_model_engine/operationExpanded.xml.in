  <Operation name="tan" exec="TRIG" syntax="Function" commutative="false" inputTailRepeat="0">
    <!-- flat, 90 or 180 degrees, rounded problem conver from degrees to radians -->
    <SameUn onlyDomainIll="(v==-1||com::equal_epsilon(M_PI*0.5,(double)v)||com::equal_epsilon(M_PI*1.5,(double)v))" function="std::tan(v)" foTypes="f"/>
    <Result>
