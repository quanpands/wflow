 -->
  <!-- # if/mif looks like function but it is -->
  <!-- # a grammar construction, do not insert in symbol table -->
  <!-- # if without an else clause is a spatial mask -->
  <!-- # normal use assumes one of the argument to be spatial -->
  <!-- # hence the result is spatial -->
  <Operation name="if" implName="ifthen" syntax="None" exec="IFTHEN" commutative="false" inputTailRepeat="0">
    <Manual/>
    <Result>
      <Field spatial="Yes">
        <DataType value="Boolean"/>
        <DataType value="Nominal"/>
        <DataType value="Ordinal"/>
        <DataType value="Scalar"/>
        <DataType value="Directional"/>
        <DataType value="Ldd"/>
      </Field>
    </Result>
    <Input repeat="false">
      <Field spatial="Either">
        <DataType value="Boolean"/>
      </Field>
    </Input>
    <Input repeat="false">
      <Field spatial="Either">
        <DataType value="Boolean"/>
        <DataType value="Nominal"/>
        <DataType value="Ordinal"/>
        <DataType value="Scalar"/>
        <DataType value="Directional"/>
        <DataType value="Ldd"/>
      </Field>
    </Input>
  </Operation>
